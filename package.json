    "start": "npm run build && node .",
    "build": "tsc",
    "test": "jest --detectOpenHandles",
    "build-and-test": "npm run build && npm run test",
    "lint": "eslint ./src ./postgres-db -c .eslintrc.json --ext ts,tsx --no-error-on-unmatched-pattern --no-ignore",
    "lint-fix": "eslint ./src/** ./postgres-db/** -c .eslintrc.json --fix --ext ts,tsx --no-error-on-unmatched-pattern",
    "presubmit": "npm run lint && npm run build-and-test",
    "setup": "docker-compose -f docker-compose.yml up -d",
    "tear-down": "docker-compose -f docker-compose.yml rm --force --stop",
    "migration-generate": "ts-node postgres-db/scripts/cli.ts migration:generate -n initial-artist-db --pretty",
    "migration-run": "ts-node postgres-db/scripts/cli.ts migration:run"
  },
